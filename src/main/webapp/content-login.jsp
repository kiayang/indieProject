
<FORM ACTION="j_security_check" METHOD="POST">

<table cellspaceing="5" border="0">
<tr>
<td align="left">User Name: </td>
<td><input TYPE="TEXT" NAME="j_username" maxlength="40" size="40" placeholder="enter email" pattern="[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$" title="User ID must be an email address" required>
<td/>
</tr>

<tr>
<td align="left">Password: </td>
<td><INPUT TYPE="PASSWORD" NAME="j_password" maxlength="15" maxsize="15" placeholder="enter password"required pattern=".{6,}" title="Password must be 6 characters or more!">
&nbsp;&nbsp;
<input type="submit" value="login"style="font-size:15pt;color:white;background-color:grey;border:2px solid #090566;padding:2px">
</td>
<tr/>

<br/><br/>
</table>
</form>
