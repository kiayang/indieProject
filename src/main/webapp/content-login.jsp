
<FORM ACTION="j_security_check" METHOD="POST">
<table cellspaceing="5" border="0">
<tr>
<td align="left">User ID: </td>
<td><input TYPE="TEXT" NAME="j_username" maxlength="40" size="40" pattern="[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$" title="User ID must be an email address" required>
<td/>
</tr>

<tr>
<td align="left">Password: </td>
<td><INPUT TYPE="PASSWORD" NAME="j_password" maxlength="15" maxsize="15" required pattern=".{6,}" title="Password must be 6 characters or more!">
<input type="submit" value="login"style="font-size:15pt;color:white;background-color:green;border:4px solid #336600;padding:4px">
</td>
<tr/>

<br/><br/>
</table>
</form>
